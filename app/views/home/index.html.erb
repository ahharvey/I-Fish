
<div class="container-fluid">
  <div class="row">
    <div class="col-xs-12 col-md-4 col-md-push-8">
      <% if admin_signed_in? %>
        <br />
        <br />
        <br />
        <br />
        <div id="quick_links" class="panel panel-default">
          <div class="panel-heading">
            <h3 class="panel-title">Quick Links</h3>
          </div>
          <div class="panel-body">
            <dl>
              <dt>My fisheries</dt>
              <% for fishery in current_admin.member_fisheries %> 
                <dd><%= link_to fishery.name, fishery %></dd>
              <% end %>
            </dl>
          </div>
        </div>
      <% end %>
    </div>
    <div id="recent_activity" class="col-xs-12 col-md-8 col-md-pull-4">
      <div class="page-header text-center">
          <h3 id="timeline">Recent Activity</h3>
      </div>

      
      <ul class='timeline'>
        <% @activities.each do |activity| %>
          <%= ActivityPresenter.new(activity, self).render_activity %>
        <% end %>
        <li class="clearfix" style="float: none;"></li>
      </ul>
      
      
      <div class="row">
        <div class="pull-right">
          <%= render partial: 'home/pagination' %>
          
        </ul>
        
      </div>
    </div>
    
    
  </div>
</div>


