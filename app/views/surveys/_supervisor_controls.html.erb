<div class="span2">
  <%= link_to supervisor_dashboard_index_path  do %>
  	<i class='icon-arrow-left'></i> Back to Dashboard 
  <% end %>
</div>
<div class="span1 offset2">
  <b>Approved:</b>
</div>
<div class="span1">
  <div class="controls switch switch-large" data-on= "success" data-off="danger" data-on-label="<i class='icon-ok icon-white icon-large'></i>" data-off-label= "<i class='icon-remove icon-white icon-large'></i>">
  	<%= check_box_tag :approved, @survey.id, @survey.approved?, class: 'approve-toggle', data: { href: set_approved_survey_path(@survey) } %>
  </div>
</div>