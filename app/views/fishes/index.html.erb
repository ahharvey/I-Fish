<h1>Listing fishes</h1>

<table  class="table table-bordered table-striped">
  <tr>
    <th>Order</th>
    <th>Family</th>
    <th>Scientific name</th>
    <th>Fishbase name</th>
    <th>English name</th>
    <th>Indonesia name</th>
    <th>Code</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @fishes.each do |fish| %>
  <tr>
    <td><%= fish.order %></td>
    <td><%= fish.family %></td>
    <td><%= fish.scientific_name %></td>
    <td><%= fish.fishbase_name %></td>
    <td><%= fish.english_name %></td>
    <td><%= fish.indonesia_name %></td>
    <td><%= fish.code %></td>
    <td><%= link_to 'Show', fish %></td>
    <td><%= link_to 'Edit', edit_fish_path(fish) %></td>
    <td><%= link_to 'Destroy', fish, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Fishes', new_fish_path %>
