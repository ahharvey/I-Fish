<% title t ".title" %>

<%= simple_form_for(resource, :as => resource_name, :url => registration_path(resource_name), :html => {:method => :put, :multipart => true, class: 'form form-horizontal'}) do |f| %>
    <%= devise_error_messages! %>
    
    <div class="row">
			<div class="span6">
				<%= f.input :name %>

				<%= f.input :email %>

				<%= f.association :office %>

			</div>
		</div>

    <div class="form-actions">
      <%= content_tag :button, type: :submit, class: 'btn btn-primary pull-right', style:"margin-left:20px" do %>
        Save
      <% end %>
      <%= link_to "Cancel", :back, class: "btn pull-right" %>
    </div>

<% end %>
