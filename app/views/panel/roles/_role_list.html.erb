<div id="RoleList" >
  
  <ul>
    <% @roles.each do |role| %>
      <li>
        <%= role.name %>
        <% if can? :destroy, role %>
          <%= link_to remove_icon, [:panel, role], method: :delete, confirm: 'You sure?', remote: true %>
        <% end %>
      </li>
    <% end %>
  </ul>

</div>