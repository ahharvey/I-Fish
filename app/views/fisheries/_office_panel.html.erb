<div class="panel panel-default">
  <div class="panel-heading">
    <h3 class="panel-title">Supporting Offices</h3>
  </div>
  <div class="panel-body">
    <% if can? :update, @fishery %>
      <%= form_tag [:add_member_office, @fishery], class: "form", :remote=>true do %>
        <div class="input-group"> 
          <%= text_field_tag :get_office, "", class: "form-control", placeholder: "Add an office ...", autocomplete: "off", data: { autocomplete_source: Office.order(:name).map(&:name) } %>
          <div class="input-group-btn">
            <%= button_tag add_icon, class: "btn btn-success" %>
          </div>
        </div>
      <% end %>

    <% end %>
    <%= render partial: 'office_list' %>
  </div>
</div>
</div>