<% title "Activity Feed" %>




<div class="container">
	<div class="row">
		<div class="col-xs-12 col-md-6 col-md-offset-3">
			<h5>Recent Activity</h5>

			
			<div id="surveys">
				<% @activities.each do |activity| %>
					<%= ActivityPresenter.new(activity, self).render_activity %>
				<% end %>
			</div>
			
			
		</div>
		
	</div>
</div>